{
  "expo": {
    "name": "Interclip",
    "slug": "iclip",
    "version": "0.10.1",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/filiptronicek/iclip-mobile",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "icon": "./assets/ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.filiptronicek.iclip",
      "buildNumber": "0.10.1",
      "infoPlist": {
        "NSCameraUsageDescription": "Interclip uses your camera to scan QR codes. Interclip will also use your camera if you upload a file with the Camera source.",
        "NSMicrophoneUsageDescription": "Interclip uses your microphone to record audio when you record video through the file upload page.",
        "NSPhotoLibraryUsageDescription": "Interclip uses your photo library to allow photo and video uploads.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.filiptronicek.iclip",
      "permissions": [
        "CAMERA",
        "VIBRATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 32
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A mobile client for Interclip"
  }
}
